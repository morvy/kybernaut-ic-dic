{
	"landingPage": "/",
	"preferredVersions": {
		"php": "8.0",
		"wp": "latest"
	},
	"features": {
		"networking": true
	},
	"steps": [
		{
			"step": "installTheme",
			"themeData": {
				"resource": "wordpress.org/themes",
				"slug": "twentytwentyfour"
			}
		},
		{
			"step": "importWordPressFiles",
			"wordPressFilesZip": {
				"resource": "url",
				"url": "https://kybernaut.cz/blueprints/kybernaut-ico-dic-playground.zip"
			}
		},
		{
			"step": "wp-cli",
			"command": "wp plugin update --all"
		},
		{
			"step": "login",
			"username": "admin",
			"password": "password"
		}
	]
}
